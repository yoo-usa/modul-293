{% embed '@craft-components/navbar/navbar.twig' %}
    {% block navbarMobileHeaderContent %}
        <a href="{{ siteUrl('') }}">Logo</a>
        {{ parent() }}
    {% endblock %}
    {% block navbarMobileContentBefore %}
        <div style=" height: 100px; background-color: green;"><h4>Beispiel inhalt mobile</h4></div>
    {% endblock %}
    {% block navbarMobileContentAfter %}
        <div style=" height: 100px; background-color: yellow;"><h4>Beispiel inhalt mobile</h4></div>
    {% endblock %}
    {% block desktopContent %}
        <div class="navbar-desktop" style="background-color:lightgrey; width: 100%;">
            <div>Logo</div>
            <ul class="navbar-desktop__links">
                <li><a>Home</a></li>
                <li><a>About</a></li>
            </ul>
        </div>
    {% endblock %}
{% endembed %}
